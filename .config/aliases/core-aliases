echo "core aliases sourced"
alias zsrc="source ~/.zshrc"
alias zrc="$EDITOR"" ~/.zshrc"
alias config="$EDITOR"" ~/.config/"
alias variables="$EDITOR"" ~/.config/environment-variables"
alias aliases="$EDITOR"" ~/.config/aliases"
alias vim="nvim"
alias lua="$EDITOR"" ~/.config/nvim/lua"
alias vrc="$EDITOR"" ~/.config/nvim/init.lua"
alias readme="$EDITOR"" ~/readme.txt"
alias l='ls -larth'
